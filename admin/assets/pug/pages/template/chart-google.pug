- var page_Title = 'Google Chart';
- var theme_customizer      = true;
- var googlechart           = true;
- var page_googlechart      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
include ../../components/mixin
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        +breadcrumb(['Charts','Google Chart'],'Google Chart')
        include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          // Container-fluid starts
          .container-fluid
            .row

              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Area Chart 
                      span 1
                  .card-body.p-0.chart-block
                    #area-chart1.chart-overflow
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Area Chart 
                      span 2
                  .card-body.p-0.chart-block
                    #area-chart2.chart-overflow
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Column Chart 
                      span 1 
                  .card-body.chart-block
                    #column-chart1.chart-overflow
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Column Chart 
                      span 2
                  .card-body.chart-block
                    #column-chart2.chart-overflow
              .col-sm-12.box-col-12
                .card
                  .card-header
                    h4 Gantt Chart
                  .card-body.chart-block
                    #gantt_chart.chart-overflow
              .col-sm-12.box-col-12
                .card
                  .card-header
                    h4 Line Chart
                  .card-body.chart-block
                    #line-chart.chart-overflow
              .col-sm-12.box-col-12
                .card
                  .card-header
                    h4 Combo Chart
                  .card-body.chart-block
                    #combo-chart.chart-overflow
              .col-sm-12.col-xl-6.box-col-12
                .card
                  .card-header
                    h4 bar-chart2
                  .card-body.chart-block
                    #bar-chart2
              .col-sm-12.col-xl-6.box-col-12
                .card
                  .card-header
                    h4 word tree
                  .card-body.chart-block
                    #wordtree_basic.word-tree

              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Pie Chart 
                      span 1
                  .card-body.p-0.chart-block
                    #pie-chart1.chart-overflow
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Pie Chart 
                      span 2
                  .card-body.p-0.chart-block
                    #pie-chart3.chart-overflow
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Pie Chart 
                      span 3
                  .card-body.p-0.chart-block
                    #pie-chart4.chart-overflow

              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Pie Chart 
                      span 4
                  .card-body.p-0.chart-block
                    #pie-chart2.chart-overflow

          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
